class Vector{
    init(a,b,c){
        this.a = a;
        this.b = b;
        this.c = c;
    }

    plus_(other){
        assert(type(other)==type(this), "second operand must be Vector");
        return Vector(this.a+other.a, this.b+other.b, this.c+other.c);
    }

    asString(){
        return "["+this.a+", "+this.b+", "+this.c+"]";
    }
}

var v1 = Vector(1, 2, 3);
var v2 = Vector(2,4, -7);

print (v1+v2).asString();
